           change24: body['change24'][0] + body['change24'][1],\n                        high: body['high'],\n                        low: body['low'],\n                        supply: body['supply'][0] + body['supply'][1],\n                        ath: body['ath'],\n                        atl: body['atl'],\n                        maxSupply: maxSupply_mod\n                    });\n                } else {\n                    console.log('Null Response!');\n                }\n            })\n            .catch( function(error) {\n                // Log error\n                console.log(error);\n            });\n    };\n\n    // Fade in objects\n    async fadeIn() {\n        // Fade in elements\n        let faders = document.querySelectorAll('.stat-row');\n        // Timer to space out fade\n        let timer = (ms) => new Promise(res => setTimeout(res, ms));\n\n        // For each element\n        for(let i = 0; i < faders.length; i++) {\n            // Fade in every 700 ms\n            await timer(500);\n            faders[i].classList.add('is-visible');\n        };\n    };\n\n    // On Component Mount\n    componentDidMount = () => {\n        // Update values\n        this.loadData();\n        this.fadeIn();\n    }\n\n    render() {\n        return (\n            <div className=\"EthStats stats\">\n                <Container className=\"mt-4\">\n                    <Row>\n                        <Col sm={8}>\n                            <div className=\"stats-head\"> \n                                <h1> Ethereum </h1>\n                                <h4> (ETH) </h4>\n                            </div>\n                        </Col>\n                        <Col sm={4} className=\"my-auto\">\n                            <h2> ${this.state.price} </h2>\n                        </Col>\n                    </Row>\n                </Container>\n                <Container className=\"mt-5\">\n                    <Row className=\"stat-row\">\n                      