           window.scrollTo({\n                top: 0,\n                left: 0,\n                behavior: 'instant'\n            });\n        });\n        return () => {\n            unlisten();\n        }\n    }, []);\n    \n    return(null);\n}\n\nexport default withRouter(ScrollToTop);","export default \"data:image/png;base64,\"","export default \"data:image/png;base64,\"","export default \"data:image/png;base64,\"","export default __webpack_public_path__ + \"static/media/site-logo.8c6f1077.png\";","// Navbar Component -- Top of page\nimport { Navbar } from 'react-bootstrap';\nimport NavbarCollapse from 'react-bootstrap/esm/NavbarCollapse';\n// React Router\nimport { NavLink } from 'react-router-dom';\n// Images for navigation\nimport ETH from '../../img/nav/eth.png';\nimport BTC from '../../img/nav/btc.png';\nimport ADA from '../../img/nav/ada.png';\nimport Site from '../../img/nav/site-logo.png';\n\nconst Navi = () => {\n    return (\n        <div className=\"Navi\">\n            <Navbar expand=\"lg\">\n                <NavbarCollapse className=\"justify-content-center\">\n                    <div className=\"NavItem\"> \n                            <NavLink exact to=\"/\"> \n                                <img src={ Site } alt=\"Site Home\"\n                                    height=\"48\"\n                                />\n                            </NavLink>\n                        </div>\n                        \n                        <div className=\"NavItem\"> \n                            <NavLink exact to=\"/ethereum\"> \n                                <img src={ ETH } alt=\"Ethereum\"\n                                    height=\"48\" width=\"48\"\n                                />\n                            </NavLink>\n                        </div>\n                        <div className=\"NavItem\"> \n                            <NavLink exact to=\"/bitcoin\"> \n                                <img src={ BTC } alt=\"Bitcoin\"\n       